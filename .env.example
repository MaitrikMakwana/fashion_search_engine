# Copy this file to .env and fill your keys
# Never commit .env to Git

# Google Gemini API
GEMINI_API_KEY=

# SerpAPI key
SERPAPI_API_KEY=

# Server port (defaults to 5000)
PORT=5001

# 1/true => mock responses (no external calls)
MOCK=0

# 1/true => Gemini only corrects spelling and returns corrected phrase
SPELL_ONLY=1